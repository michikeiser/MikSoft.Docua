<Window x:Class="MikSoft.Docua.Client.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MikSoft.Docua.Client"
        xmlns:regions="http://prismlibrary.com/"
        xmlns:constants="clr-namespace:MikSoft.Docua.Client.Constants"
        mc:Ignorable="d"
        Title="ShellView" Height="450" Width="800"
        regions:ViewModelLocator.AutoWireViewModel="True"
        d:DataContext="{d:DesignInstance local:ShellViewModel}">
    <Grid>
        <ContentControl regions:RegionManager.RegionName="{x:Static constants:RegionNames.REGION_SHELL_CONTENT}"/>
    </Grid>
</Window>